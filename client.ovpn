client
proto tcp-client
dev tun
persist-key
persist-tun

remote-cert-tls server

remote <vpn server ip address> 1194
nobind
comp-lzo
keepalive 60 600
sndbuf 393216
rcvbuf 393216

verb 0

user nobody
group nogroup

ca /etc/openvpn/client/ca.crt
cert /etc/openvpn/client/client.crt
key /etc/openvpn/client/client.key
cipher AES-256-CBC
